{
  "name": "@andr-ll/tenv",
  "version": "1.1.3",
  "author": "andr_ll",
  "license": "ISC",
  "description": "An utility for creation types for `process.env` variables and `.env` file",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "tenv": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:andr-ll/tenv.git"
  },
  "scripts": {
    "start": "ts-node ./src",
    "build": "run-s build:lib build:bin",
    "build:lib": "tsc",
    "build:bin": "./scripts/build.sh",
    "types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "format:check": "prettier --check '**/*.{js,json,md,ts,yml,yaml}'",
    "lint": "eslint --fix '**/*.ts'",
    "lint:check": "eslint '**/*.ts'",
    "spell": "cspell '**/*'",
    "husky:init": "git config core.hooksPath '.husky'",
    "version:check": "ts-node ./scripts/version_check.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cspell": "^6.8.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "@andr-ll:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
