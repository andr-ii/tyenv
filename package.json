{
  "name": "typedenv",
  "version": "1.1.8",
  "author": "Andrii Lytovchenko <andr.lyt.dev@gmail.com>",
  "license": "MIT",
  "description": "An utility for creation types for `process.env` variables and `.env` file",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "typedenv": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "javascript",
    "environment variables"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:andr-ii/typedenv.git"
  },
  "scripts": {
    "start": "ts-node ./src",
    "build": "run-s build:lib build:bin",
    "build:lib": "tsc",
    "build:bin": "npsh bin 'init()'",
    "types": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,json,md,ts,yml,yaml}'",
    "format:check": "prettier -l '**/*.{js,json,md,ts,yml,yaml}'",
    "lint": "eslint --fix '**/*.ts'",
    "lint:check": "eslint '**/*.ts'",
    "spell": "cspell '**/*'",
    "hooks": "npsh hooks",
    "svc": "npsh svc andr-ii typedenv master"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cspell": "^6.8.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "npsh": "^2.0.0",
    "npm-run-all": "^4.1.5"
  }
}
